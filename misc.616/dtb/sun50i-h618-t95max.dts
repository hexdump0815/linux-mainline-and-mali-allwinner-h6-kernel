	opp-1560000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1560000000>;
		opp-microvolt = <1160000 1160000 1160000>;
		status = "disabled";
	};

	opp-1608000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1608000000>;
		opp-microvolt = <1160000 1160000 1160000>;
		status = "disabled";
	};

	opp-1656000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1656000000>;
		opp-microvolt = <1160000 1160000 1160000>;
		status = "disabled";
	};

	opp-1704000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1704000000>;
		opp-microvolt = <1160000 1160000 1160000>;
		status = "disabled";
	};

	opp-1752000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1752000000>;
		opp-microvolt = <1160000 1160000 1160000>;
		status = "disabled";
	};

	opp-1800000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1800000000>;
		opp-microvolt = <1160000 1160000 1160000>;
		status = "disabled";
	};
};

&gpu_opp_table {
	compatible = "operating-points-v2";
	opp-125000000 {
		opp-hz = /bits/ 64 <125000000>;
		opp-microvolt = <860000>;
		status = "okay";
	};
	opp-250000000 {
		opp-hz = /bits/ 64 <250000000>;
		opp-microvolt = <860000>;
		status = "okay";
	};
	opp-432000000 {
		opp-hz = /bits/ 64 <432000000>;
		opp-microvolt = <860000>;
		status = "okay";
	};
	opp-600000000 {
		opp-hz = /bits/ 64 <600000000>;
		opp-microvolt = <1020000>;
		status = "okay";
	};
	opp-800000000 {
		opp-hz = /bits/ 64 <800000000>;
		opp-microvolt = <1080000>;
		status = "disabled";
	};
};
