		status = "okay";
	};

	opp-1416000000 {
		clock-latency-ns = <488288>; /* 8 32k periods */
		opp-hz = /bits/ 64 <1416000000>;
		opp-microvolt = <1150000 1150000 1150000>;
		status = "okay";
